Package: NMdata
Type: Package
Title: Preparation, Checking and Post-Processing Data for PK/PD
        Modeling
Version: 0.1.7
Authors@R: 
  c(person(given="Philip", family="Delff",
     email = "philip@delff.dk",
     role = c("aut", "cre")),
    person(given="Eric", family="Anderson",
    email = "andersone@metrumrg.com",
